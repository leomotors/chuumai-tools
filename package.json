{
  "name": "chuumai-tools",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build --continue=dependencies-successful",
    "dev": "turbo run dev",
    "check": "turbo run check --continue",
    "lint": "turbo run lint --continue",
    "format": "turbo run format --continue",
    "test": "turbo run test --continue",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@leomotors/config": "^1.0.0-rc.1",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "turbo": "^2.5.2",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
